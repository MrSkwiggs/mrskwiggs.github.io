---
layout: compress
---

{% assign icon_url = "/assets/img/favicons" | relative_url %}

{
  "name": "{{ site.title }}",
  "short_name": "{{ site.title }}",
  "description": "{{ site.description }}",
  "icons": [
    {
      "src": "{{ icon_url }}/android-icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "density": "4.0"
    }],
    "start_url": "{{ '/index.html' | relative_url }}",
    "display": "fullscreen",
    "theme_color": "#2a1e6b",
    "background_color": "white"
}
